	function CH2486_winOpen(url){
		var height = '';
		var width = '';
		height = (screen.availHeight-133);
		width = (screen.availWidth-20);
		
		if( url.indexOf("http://") < 0 && url.indexOf("https://") < 0 ){
			url = "http://" + url;
		}
		
		window.open(url, '','scrollbars=yes,toolbar=yes,location=yes,status=yes,menubar=yes,resizable=yes,width='+width+',height='+height+',left=0,top=0');
	}
</script>
