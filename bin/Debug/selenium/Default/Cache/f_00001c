<script>
function CH2486_RollImage(json){
	//이미지 롤링 설정값
	var config = {
		currentImg: 0,
		nextImg 	: 1,
		listArea	: CH2486_e(json.list_area),
		imageList : CH2486_e(json.list_area).getElementsByTagName("DIV"),
		imgCnt 		: CH2486_e(json.list_area).getElementsByTagName("DIV").length-1, //0부터 시작
		rollTime 	: json.roll_time,
		moveTime 	: json.move_time,
		coordX1 	: 0,
		coordX2 	: CH2486_e(json.list_area).offsetWidth,
		coordY 		: CH2486_e(json.list_area).offsetHeight,
		moveAt 		: json.moveAt,
		direction 	: json.direction,
		label2 		: CH2486_e(json.label2),
		labelType 	: json.labelType,
		moveCnt		: CH2486_e(json.moveCnt)
	};
	//alert("gmlwh>>>e(json.list_area).offsetWidth:"+e(json.list_area).offsetWidth);
	
	CH2486_labelBind(config); //라벨(버튼) 바인드
	CH2486_setRoll(config); //롤링 시작
	CH2486_rollPause(config); //마우스 오버시 롤링 멈춤
	CH2486_moveCntBind(config);
	
	//이미지 롤링 기본 세팅하기
	function CH2486_setRoll(c){
		c.coordX1 = 0;
		c.coordX2 = c.listArea.offsetWidth;
		c.coordY = c.listArea.offsetHeight;

		if(c.direction=="right" || c.direction=="down"){
			c.coordX2 = c.coordX2 * -1;
			c.coordY = c.coordY * -1;
		}
// 		c.coordX2 = c.coordX2 - 156;//500
		
		if(c.imageList[c.nextImg] && c.imageList[c.nextImg]!=null){
			c.imageList[c.nextImg].style.display = "block";
		}
		if(c.imageList[c.nextImg] && c.imageList[c.nextImg]!=null){
		CH2486_setPosition(c);
		CH2486_rollOver(c);
		}
		//c.imageList[c.nextImg].style.left = c.coordX2+"px";
		
		c.rollTimer = setTimeout(function(){CH2486_imgMove(c)},c.rollTime);
	};

	//이미지를 움직이게 한다.
	function CH2486_imgMove(c){
		if(c.imageList[c.currentImg] && c.imageList[c.currentImg]!=null){
			if(c.direction == "left" || c.direction == "right"){
				c.imageList[c.currentImg].style.left = c.coordX1 + "px";
				c.imageList[c.nextImg].style.left = c.coordX2 + "px";
			}else if(c.direction == "up" || c.direction == "down"){
				c.imageList[c.currentImg].style.top = c.coordX1 + "px";
				c.imageList[c.nextImg].style.top = c.coordY + "px";
			}
		}
// 		alert("gmlwh>>>c.nextImg.left:"+c.imageList[c.nextImg].style.left);
// 		alert("gmlwh>>>c.coordX1:"+c.coordX1+",moveAt:"+moveAt);
		var moveAt = parseInt(c.moveAt);
		
		var nextImgLeftInt = parseInt(c.imageList[c.nextImg].style.left.replace("px", ""));
		
		/* alert("gmlwh>>>c.coordX1:"+c.coordX1+",moveAt:"+moveAt+", c.nextImg.left:"+c.nextImg.left+",nextImgLeftInt:"+nextImgLeftInt
				+"\n1: "+(nextImgLeftInt < 100)
				+"\n2: "+(moveAt != 0)
				+"\n3: "+(c.nextImg.left != 'undefined')
				+"\n4: "+(c.nextImg.left != 0)
				+"\n5: "+(!isNullStr(c.nextImg.left))
				); */
		if( (nextImgLeftInt < 100) && (moveAt != 0) && (nextImgLeftInt != 0) ){
			moveAt = nextImgLeftInt;
		}
		
		if (c.direction == "left"){
			c.coordX1 -= moveAt;
			c.coordX2 -= moveAt;
		}else if(c.direction == "right"){
			c.coordX1 += moveAt;
			c.coordX2 += moveAt;
		}else if(c.direction=="up"){
			c.coordX1 -= moveAt;
			c.coordY -= moveAt;
		}else if(c.direction=="down"){
			c.coordX1 += moveAt;
			c.coordY += moveAt;
		}
		
		//if(c.coordX1 < (-1*c.listArea.offsetWidth) ) {
		if( CH2486_isNextImgRoll(c) ) {
			c.currentImg = c.nextImg;
			c.nextImg += 1;
			if(c.currentImg == c.imgCnt) c.nextImg = 0;
			clearTimeout(c.moveTimer);
			clearTimeout(c.rollTimer);
			CH2486_setRoll(c);
			return;
		}
		c.moveTimer = setTimeout(function(){CH2486_imgMove(c)},c.moveTime);
	};
	
	//다음 이미지 롤링 해야하는지 확인
	function CH2486_isNextImgRoll(c){
		var d = c.direction;
		if(d=="left" && c.coordX2 < 0 ) return true;
		else if(d=="right" && c.coordX2 > 0) return true;
		else if(d=="up" && c.coordY < 0 ) return true;
		else if(d=="down" && c.coordY > 0) return true;

		return false
	};

	//롤링 방향에 따른 두번째 이미지 위치 좌표 설정
	function CH2486_setPosition(c){
		var d = c.direction;
		if(d=="left") c.imageList[c.nextImg].style.left = c.listArea.offsetWidth+"px";
		else if(d=="right") c.imageList[c.nextImg].style.left = (-1 * c.listArea.offsetWidth) + "px";
		else if(d=="up") c.imageList[c.nextImg].style.top = c.listArea.offsetHeight + "px";
		else if(d=="down") c.imageList[c.nextImg].style.top = (-1 * c.listArea.offsetHeight) + "px";

		//alert(c.imageList[c.nextImg].style.left);
	};
	
	//onmouseover 시 움직임 멈춤
	//플레이 버튼 추가로 인해 주석처리
	function CH2486_rollPause(c){
	//alert(c.listArea.onmouseover);
	/*
		c.listArea.onmouseover = function(){
			clearTimeout(c.rollTimer);
		}

		c.listArea.onmouseout = function(){
			CH2486_setRoll(c);
		}
	*/
	};
	
	//플레이버튼 바인드
	function CH2486_moveCntBind(c){
		if(c.moveCnt == null) return;
		
		$("#moveCnt").click(function(){
			if($("#moveCnt").attr("class") == "moveClass_on"){
				clearTimeout(c.rollTimer);
				$("#moveCnt").attr("class", "moveClass_off");
			}
			else{
				CH2486_setRoll(c);
				$("#moveCnt").attr("class", "moveClass_on");
			}
		});
	}
	
	//라벨과 바인드
	function CH2486_labelBind(c){
		if(c.label2 == null) return; //라벨을 사용하지 않으면 아래는 실행되지 않는다.
		var labels = c.label2.getElementsByTagName(c.labelType);

		c.label2.onmouseover = function(event){ //라벨영역에 마우스가 오면
			var evt = event || window.event;
			var t = evt.target || evt.srcElement;
			for(n in labels){ 
				if(labels[n] == t){
					//c.imageList[c.currentImg].style.display = "none";
					c.currentImg = parseInt(n);
					c.nextImg = parseInt(n)+1;
					if(c.currentImg == c.imgCnt) c.nextImg = 0;
					clearTimeout(c.rollTimer);
					CH2486_viewImg(c);
					CH2486_rollOver(c);
					break;
				}
			}
			//alert(event.srcElement)
		}
		
		c.label2.onmouseout = function(event){
			var evt = event || window.event;
			var t = evt.target || evt.srcElement;
			for(n in labels){
				if(labels[n]==t){
					CH2486_setRoll(c);
					break;
				}
			}
		}
	};

	//라벨 onmouseover 시 클래스 적용
	function CH2486_rollOver(c){
		if(c.label2 == null) return;
		var els = c.label2.getElementsByTagName(c.labelType);
		
		if(c.labelType == "img"){
			
			for(n in els){
				if(typeof els[n] == "object"){
					if(n == c.currentImg){
						els[n].src = els[n].getAttribute("oversrc");
					}else{
						els[n].src = els[n].getAttribute("outsrc");
					}
				}
			}
		}else{
			for(n in els){
				if(typeof els[n] == "object"){
					if(n == c.currentImg){
						var ocss = els[n].className;
						els[n].className = ocss+" "+els[n].getAttribute("overcss");
					}else{
						els[n].className = els[n].getAttribute("outcss");
					}
				}
			}
		}
	}

	//라벨에서 선택된 이미지 보이기
	function CH2486_viewImg(c){
		//alert(c.currentImg);
		for(n=0; n<c.imgCnt+1; n++)	{
			c.imageList[n].style.display = "none";
		}
		
		c.imageList[c.currentImg].style.left = "0px";
		c.imageList[c.currentImg].style.top = "0px";
		c.imageList[c.currentImg].style.display = "block";
	};
}

function CH2486_debug(t){
e("dis").innerHTML = t + "<br>";
}

//id값으로 객체 반환
function CH2486_e(id){
	var o = document.getElementById(id);
	if(typeof o == undefined || o == null) { return null;}

	return o;
}

//이미지 오류시 대체
function CH2486_setDefaultImage(imgObj){
	if(imgObj.src.indexOf("noimage.png") < 0){
		imgObj.src = _portalDir + "/img/noimage.png";
	}
}
</script>
