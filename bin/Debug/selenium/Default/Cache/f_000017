function openFloorPlansPop(){
	var params = {
			facade : "FloorFacade"
			,command : "listFloor"
			,nextpage : "/cef/floor/pop/floorPlansMain.jsp"
			,msgId : "CEF-100302" // 상암사옥 층별 안내
			,companyId : _companyId
			,menu : "userExtMenu"
	};
	
	var url = _webapp + "/XClickController?" + jQuery.param(params);
	var title = "";
	var width = 1040;
	var height = 680;
	var standard = false;
	
	openPopupWindowInCenter(url, "XClick_sessionlist", width, height, standard);
}

function toggleConfigMenu(id){
	jQuery("#"+id).toggle();
	jQuery("#BTN_DROPDOWN").toggleClass("unit_option").toggleClass("unit_option_up");
}
function toggleCompanyList(){
	jQuery("#addjobCompnyDiv").toggle();
}

function openAdminWin() {

//관리자페이지 POPUP 호출
			if( _xclick_mbcmportalmbccokr_adminWin ) {
				_xclick_mbcmportalmbccokr_adminWin.close();
			}
			var width = Math.min(window.screen.availWidth-100,1200);
			var height = Math.min(window.screen.availHeight-100,800);
			var vTop = 0;//(screen.height - height) / 2;
			var vLeft = 0;//(screen.width - width) / 2;
			var sFeatures = 'width='+width+',height='+height+',top='+vTop+',left='+vLeft+',toolbar=no,location=no,directories=no,status=yes,menubar=no,scrollbars=yes,resizable=yes';
			_xclick_mbcmportalmbccokr_adminWin = window.open(_webapp + '/sys/sysMain.jsp', '_xclick_mbcmportalmbccokr_win', sFeatures);
			_xclick_mbcmportalmbccokr_adminWin.focus();

}
	
/**
 * query
 */
function ext_query() {
	var userData;
	// MBC - 부가메뉴 사원검색 검색대상, selectBox selected대상 -> 검색대상 전체로 수정, 2016.10.24 : 박나래
// 	var srchTarget = document.getElementById("srchTarget");
	var srchKeyword = document.getElementById("srchKeyword");
// 	if(isNullStr(srchTarget.value)){
// 		srchTarget.value = "userName";
// 	}
	if(isNullStr(srchKeyword.value)){
		alert('검색어를 입력하세요');
		srchKeyword.focus();
		return;
	}
	
	jQuery.ajax({
		type : "POST",
		async : false,
		url : _webapp + "/AjaxController",
		data : {
			ajax_result_type : "JSON",
			transaction_yn : "N",
			facade : "UserFacade",
			command : "checkSearchListAjax",	
// 			srchTarget : srchTarget.value,	
			srchKeyword : $.trim(srchKeyword.value)
		},
		error : function(xhr, status, error){
			alert(error);
		},
		success : function (data) {
			userData = data;	
		}
	});
	
	if(userData.length > 1) { 	// list popup
		var params = {
				srchKeyword : $.trim(srchKeyword.value)
// 				,srchTarget : srchTarget.value
				,facade : "UserFacade"
				,command : "listSearchUser"
				,nextpage : "/acl/pop/userDetailListPop.jsp"
				,msgId : "ACL-000280" // 사용자 상세리스트
				,isPopup : 'true'
			};
			var urlStr = _webapp + "/XClickController?" + jQuery.param(params);
			// 2016-11-01 jtlim => width 변경
			//openPopupWindowInCenter(urlStr, '_extUserSrchList', 720, 670, false);
			openPopupWindowInCenter(urlStr, '_extUserSrchList', 870, 670, false);
		
	} else if(userData.length == 1){   // 사용자 상세정보
			var params = {
				userId : userData[0].userid
				,facade : "UserFacade"
				,command : "viewSearchUser"
				,nextpage : "/acl/pop/userDetailPop.jsp"
				,msgId : "ACL-000401" // 사용자 상세정보
				,isPopup : 'true'
			};
			var urlStr = _webapp + "/XClickController?" + jQuery.param(params);
			openPopupWindowInCenter(urlStr, '_extUserSrchDetail', 620, 670, false);
	}else{
		alert("검색된 사용자가 없습니다.");
	}
}

/**
 * nextProcess
 */
function nextProcess () {
	query();
}

//MBC - 한줄공지 추가, 2016.10.28 : 박나래
//한줄공지 스크립트 시작
function textScroll(scroll_el_id) {
    this.objElement = document.getElementById(scroll_el_id);
    this.objElement.style.position = 'relative';
    this.objElement.style.overflow = 'hidden';

    this.objLi = this.objElement.getElementsByTagName('li');
    this.height = this.objElement.offsetHeight; // li 엘리먼트가 움직이는 높이(외부에서 변경가능)
    this.num = this.objLi.length; // li 엘리먼트의 총 갯수
    this.totalHeight = this.height*this.num; // 총 높이
    this.scrollspeed = 1; // 스크롤되는 px
    this.objTop = new Array(); // 각 li의 top 위치를 저장
    this.timer = null;
    
    for(var i=0; this.num>1 && i<this.num; i++){
        this.objLi[i].style.position = 'absolute';
        this.objTop[i] = this.height*i;
        this.objLi[i].style.top = this.objTop[i]+"px";
    }
}

textScroll.prototype.move = function(){
    for(var i=0; i<this.num; i++) {
        this.objTop[i] = this.objTop[i] - this.scrollspeed;
        this.objLi[i].style.top = this.objTop[i]+"px";
    }
    if(this.objTop[0]%this.height == 0){
        this.jump();
    }else{
        clearTimeout(this.timer);
        this.timer = setTimeout(this.name+".move()",50);
    }
}

textScroll.prototype.jump = function(){
    for(var i=0; i<this.num; i++){
        if(this.objTop[i] == this.height*(-1)){
            this.objTop[i] = this.objTop[i] + this.totalHeight;
            this.objLi[i].style.top = this.objTop[i]+"px";
        }
    }
    clearTimeout(this.timer);
    this.timer = setTimeout(this.name+".move()",4000);
}

textScroll.prototype.start = function() {
    this.timer = setTimeout(this.name+".move()",4000);
}

var real_search_keyword = new textScroll('scrollNoti'); // 스크롤링 하고자하는 ul 엘리먼트의 id값을 인자로 넣습니다
real_search_keyword.name = "real_search_keyword"; // 인스턴스 네임을 등록합니다
real_search_keyword.start(); // 스크롤링 시작
//한줄공지 스크립트 끝
//-->
</script>
