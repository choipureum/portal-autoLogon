라우저에서 팝업시 포커스가 되지 않는 문제로 제거함 )	if(navigator.userAgent.indexOf("Edge") == -1){		pop.focus();	}		if(xClickTop._popupWindowsMap){		xClickTop._popupWindowsMap.set('', pop);	}	return pop;}/*  MBC / 김영란법 서약 동의서 팝업 / 2017. 02. 10. hjkim*/function kyrLawAgreePop() {	var url = _webapp + "/AjaxController?ajax_result_type=JSON";	var params = {		facade : "CefNotifyFacade",		command : "getKYRAgreeUser",		empno : _sabun	};		jQuery.ajax({		url : url,		async: true,		data : params,		error : function(xhr, status, error){ alert("[kyrLawAgreePop Call Error] "+ error); },		success : function (data){			if(data.popupYn == 'Y'){				var width = "1024";				var height = "768";				var vTop = ((screen.height - height) / 2)-100;				var vLeft = (screen.width - width) / 2;				var sFeatures = 'width='+width+',height='+height+',top='+vTop+',left='+vLeft+',toolbar=no,location=no,directories=no,status=yes,menubar=yes,scrollbars=yes,resizable=no';				var win = window.open(_webapp+"/cef/notify/cefKYRAgreePop.jsp?empno="+_sabun, "kyr_agree_pop", sFeatures);				win.focus();			}else{}		}	});	};//2017.07.12 본사 정직원 성희롱 교육  팝업 author joovar sEduWin;function closePopupNotToday(){	             	setCookie('notToday','Y', 1);	sEduWin.close();}function setCookie(name, value, expiredays) {	var today = new Date();	today.setDate(today.getDate() + expiredays);		document.cookie = name + '=' + escape(value) + '; path=/; expires=' + today.toGMTString() + ';'}function getCookie(name) { 	var cName = name + "="; 	var x = 0; 	while ( x <= document.cookie.length ){ 	    var y = (x+cName.length); 	    if ( document.cookie.substring( x, y ) == cName ) 	    { 	        if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 ) 	            endOfCookie = document.cookie.length;	        return unescape( document.cookie.substring( y, endOfCookie ) ); 	    } 	    x = document.cookie.indexOf( " ", x ) + 1; 	    if ( x == 0 ) 	        break; 	} 	return "N"; }//2017.07.12 본사 정직원 성희롱 교육 이수 확인 author joofunction sEduPop(){	var width = "388";	var height = "320";	var vTop = 100;	var vLeft = 150;	var sFeatures = 'width='+width+',height='+height+',top='+vTop+',left='+vLeft+',toolbar=no,location=no,directories=no,status=yes,menubar=yes,scrollbars=yes,resizable=no,menubar=no';	sEduWin = window.open(_webapp+"/cef/notify/cefSexEducationPop.jsp?&empno="+_sabun, "sex_edu_pop", sFeatures);	sEduWin.focus();}//2018.1.9 본사 정직원 김영란법 교육 팝업function sEduPop2(){	var width = "388";	var height = "320";	var vTop = 100;	var vLeft = 150;	var sFeatures = 'width='+width+',height='+height+',top='+vTop+',left='+vLeft+',toolbar=no,location=no,directories=no,status=yes,menubar=yes,scrollbars=yes,resizable=no,menubar=no';	sEduWin = window.open(_webapp+"/cef/notify/cefKYREducationPop.jsp?&empno="+_sabun, "kyr_edu_pop", sFeatures);	sEduWin.focus();}</script></html>