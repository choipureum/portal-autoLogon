<script language='javascript'>
	function dialIpt(calledNumber){
			jQuery.ajax({
				url : _webapp+'/AjaxController?facade=CefIPTDialFacade&command=dialIptAjax',
				data: {"calledNumber":calledNumber, "userId":"MBC1AL855c165be41f1351e3f7108800b4b1cc", "transaction_yn":"Y"},
				error 	: function(xhr, status, error){
					alert(error);
				},
				success : function (data){
					//해당 사용자에 대한 전송 결과( 등록된 사용자인가 체크만 해서 넘겨줌)
					if(data.result == 'Success'){
						messagePop("CEF-100133", "ALERT", 300, 170); //성공적으로 전화연결되었습니다.
					}else if(data.result == 'iUC_RC_USER_NOT_FOUND'){
						messagePop("CEF-100132", "ALERT", 300, 170); //등록된 사용자가 아닙니다.
					}else if(data.result == 'iUC_RC_CHANNEL_NOT_FOUND'){
						messagePop("CEF-100134", "ALERT", 300, 170); //없는 번호이거나 등록되지 않는 번호입니다.
					}else if(data.result == 'iUC_RC_INVALID_EXTENSION'){
						messagePop("CEF-100135", "ALERT", 300, 170); //CM에 등록되지 않는 전화번호입니다.
					}else{
						messagePop("CEF-100136", "ALERT", 300, 170); //전화걸기를 실패하였습니다. 상대방이 통화할 수 없는 상태입니다.
					}
				}
			});
		
	}
