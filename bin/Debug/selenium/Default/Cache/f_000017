<script type="text/javascript">
/**
* 회사ID 리턴
*/
getCompanyId = function (dom) {
	var oSelect = document.getElementById('companySelect_' + dom);
	if( oSelect ) {
		return oSelect.value;
	}else{
		return 'IM';
	}
};
/**
* 회사이름 리턴
*/
getCompanyName = function (dom) {
	var oSelect = document.getElementById('div_companySelect_' + dom);
	if( oSelect ) {
		return oSelect.innerHTML;
	}else{
		return 'iMBC';
	}
};
</script>


	    
