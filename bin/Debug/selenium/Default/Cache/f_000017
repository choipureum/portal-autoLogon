	//사원검색 값 입력시 컬러변경 : 박나래
	function chgColor(obj){
		if(obj.value!=""){
			obj.style.color = "#000000";
		}else{
			obj.style.color = "#A6A6A6";
		}
	}
	
/**
 * TEL popup
 */
function openTelPop(){
	var params = {
			facade : ""
			,command : ""
			,nextpage : "/common/pop/telMain.jsp"
			,msgId : "COMMON-000284" // TEL
			,companyId : _companyId
	};
	
	var url = _webapp + "/XClickController?" + jQuery.param(params);
	var title = "";
	var width = 920;
	var height = 680;
	var standard = false;
	
	openPopupWindowInCenter(url, "XClick_tellist", width, height, standard);
}

/**
 * session info popup
 */
function openSessionInfoPop(){
	var params = {
			facade : ""
			,command : ""
			,nextpage : "/common/pop/sessionInfoMain.jsp"
			,msgId : "COMMON-000287" // 사용자현황
			,companyId : _companyId
	};
	
	var url = _webapp + "/XClickController?" + jQuery.param(params);
	var title = "";
	var width = 940;
	var height = 680;
	var standard = false;
	
	openPopupWindowInCenter(url, "XClick_sessionlist", width, height, standard);
}

